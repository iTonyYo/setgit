{
  "name": "setgit-cli",
  "description": "配置 Git。",
  "bin": {
    "setgit": "esm/main.js"
  },
  "scripts": {
    "cli": "yarn build >/dev/null && ./esm/main.js",
    "build": "npx babel-node ./scripts/clean-built && npx babel-node ./scripts/build",
    "clean": "npx babel-node ./scripts/clean",
    "license": "npx babel-node ./scripts/license",
    "reprepare": "npx babel-node ./scripts/re-prepare",
    "lint": "npx eslint --cache .",
    "test": "cross-env NODE_ENV=test npx babel-node ./scripts/test",
    "changelog": "npx auto-changelog -p",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "files": [
    "esm/**/*.js"
  ],
  "dependencies": {
    "async": "^3.1.0",
    "chalk": "^2.4.2",
    "cosmiconfig": "^5.2.1",
    "deepmerge": "^4.0.0",
    "gradient-string": "^1.2.0",
    "is-git-repository": "^1.1.1",
    "meow": "^5.0.0",
    "nodegit": "^0.26.0",
    "redent": "^3.0.0",
    "update-notifier": "^3.0.1",
    "snyk": "^1.226.2"
  },
  "version": "1.0.2",
  "private": false,
  "git": {
    "core.editor": "code"
  },
  "gitignore": [
    "macOS",
    "Windows",
    "Linux",
    "Node",
    "VisualStudioCode",
    "SublimeText",
    "CVS",
    "Diff",
    "Vim",
    "TortoiseGit",
    "Diff",
    "Patch"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "add-module-exports",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "no-console": 0,
      "no-useless-catch": 0
    }
  },
  "eslintIgnore": [
    "esm/**",
    ".github/**",
    ".vscode/**",
    "docs/**"
  ],
  "nyc": {
    "check-coverage": false,
    "per-file": true,
    "reporter": [
      "html"
    ],
    "report-dir": "./docs/coverage",
    "cache": false,
    "exclude": [
      "**/?(*.)+(spec|test).[tj]s?(x)",
      "test/**/*.js"
    ],
    "skip-empty": true,
    "skip-full": true,
    "clean": true
  },
  "mocha": {
    "extension": [
      "js"
    ],
    "timeout": 120000,
    "ui": "tdd",
    "color": true,
    "delay": true,
    "fullStackTrace": true,
    "checkLeaks": true,
    "retries": 2
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "changelog-template.hbs",
    "unreleased": true,
    "commitLimit": false,
    "includeBranch": [
      "master"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/register": "^7.6.0",
    "auto-changelog": "^1.16.1",
    "babel-eslint": "^10.0.3",
    "babel-plugin-add-module-exports": "^1.0.2",
    "cross-env": "^5.2.1",
    "eslint": "^6.3.0",
    "execa": "^2.0.4",
    "fast-glob": "^3.0.4",
    "license-checker": "^25.0.1",
    "listr": "^0.14.3",
    "make-dir": "^3.0.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "p-map": "^3.0.0",
    "trash": "^6.0.0",
    "write-file-atomic": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/iTonyYo/setgit-cli.git"
  },
  "keywords": [
    "git",
    "setgit",
    "cli",
    "脚手架",
    "scaffold",
    "工具",
    "tool",
    "oopsunome"
  ],
  "author": {
    "name": "iyowei",
    "email": "swzyocowboy@icloud.com",
    "url": "https://github.com/iTonyYo"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iTonyYo/setgit-cli/issues"
  },
  "homepage": "https://github.com/iTonyYo/setgit-cli#readme",
  "publishConfig": {
    "access": "public"
  },
  "snyk": true
}
